{
    "name": "etrias/paazl-connector",
    "description": "Package generated from https://ost.paazl.com/parcelshipperservice/orderRequest.wsdl using wsdltophp/packagegenerator",
    "require": {
        "php": ">=5.3.3",
        "ext-soap": "*",
        "phpro/soap-client": "~0.5",
        "http-interop/http-factory": "~0.2",
        "guzzlehttp/guzzle": "~6.2",
        "http-interop/http-factory-guzzle": "~0.1"
    },
    "autoload": {
        "psr-4": {
            "Etrias\\PaazlConnector\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Etrias\\PaazlConnector\\": "./tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.4",
        "phpunit/phpunit": "^6.2",
        "zendframework/zend-code": "^3.0.4"
    },
    "scripts": {
        "fix-style": [
            "vendor/bin/php-cs-fixer fix --diff --show-progress run-in"
        ]
    },
    "authors": [
        {
            "name": "Christ-Jan",
            "email": "christ-jan@etrias.nl"
        }
    ],
    "minimum-stability": "stable"
}
